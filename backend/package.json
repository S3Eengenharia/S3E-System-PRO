{
  "name": "s3e-engenharia-backend",
  "version": "1.0.0",
  "description": "API backend para o S3E System PRO",
  "main": "dist/app.js",
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc --project tsconfig.build.json",
    "start": "node dist/app.js",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "seed": "npx tsx prisma/seed.ts",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:deploy:prod": "dotenv -e .env.production -- npx prisma migrate deploy",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate"
  },
  "keywords": [
    "api",
    "express",
    "typescript",
    "s3e"
  ],
  "author": "S3E Engenharia",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/pdfkit": "^0.17.3",
    "@types/xml2js": "^0.4.14",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.6.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "fast-xml-parser": "^5.3.2",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.8",
    "papaparse": "^5.5.3",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "prisma": "^6.17.1",
    "puppeteer": "^24.29.1",
    "sharp": "^0.34.5",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^6.4.14",
    "dotenv-cli": "^10.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
